{
    "name": "Invertify",
    "version": "0.0.1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "popup.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Invertify",
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "icons":{
        "128":"logo.png"
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs"
    ]
}